{
  "name": "@davidyang2149/nextjs-starter",
  "version": "3.0.6",
  "description": "Next.js starter with TypeScript, Jest, Testing-Library, Cypress",
  "bin": {
    "@davidyang2149/nextjs-starter": "./bin/create-nextjs-starter.js"
  },
  "scripts": {
    "dev": "next dev --turbo",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest --watchAll --coverage --detectOpenHandles",
    "test:unit": "jest",
    "cypress": "cypress open",
    "cypress:headless": "cypress run",
    "e2e": "start-server-and-test 'pnpm run build && pnpm run start' http://127.0.0.1:3000 'pnpm run cypress'",
    "e2e:headless": "start-server-and-test 'pnpm run build && pnpm run start' http://127.0.0.1:3000 'pnpm run cypress:headless'",
    "prepare": "husky install"
  },
  "dependencies": {
    "commander": "^10.0.0",
    "jotai": "^2.0.3",
    "next": "13.3.4",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@swc/core": "^1.3.36",
    "@swc/jest": "^0.2.24",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/jest-plugin-context": "^2.9.5",
    "@types/node": "18.14.1",
    "@types/react": "18.2.0",
    "@types/react-dom": "18.2.1",
    "@types/supertest": "^2.0.12",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "autoprefixer": "^10.4.14",
    "cypress": "^12.7.0",
    "eslint": "8.39.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-next": "13.3.4",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.4.3",
    "jest-environment-jsdom": "^29.4.3",
    "jest-plugin-context": "^2.9.0",
    "postcss": "^8.4.21",
    "start-server-and-test": "^2.0.0",
    "supertest": "^6.3.3",
    "tailwindcss": "^3.2.7",
    "typescript": "5.0.4",
    "husky": "^8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DavidYang2149/nextjs-starter.git"
  },
  "keywords": [
    "Next.js",
    "nextjs",
    "Next.js starter",
    "nextjs starter",
    "starter",
    "typescript",
    "eslint",
    "jest",
    "testing-library",
    "cypress"
  ],
  "author": "davidyang2149",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DavidYang2149/nextjs-starter/issues"
  },
  "homepage": "https://github.com/DavidYang2149/nextjs-starter#readme"
}
